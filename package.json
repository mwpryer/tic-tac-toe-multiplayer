{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "private": true,
  "description": "Multiplayer Tic-Tac-Toe game",
  "license": "ISC",
  "author": "Matt Pryer <mwpryer@gmail.com>",
  "scripts": {
    "build": "tsc",
    "client": "npm run dev --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "tsc && cd client && npm install && npm run build ",
    "server": "nodemon src/index.ts",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "socket.io": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/express": "^4.17.13",
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.18",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
